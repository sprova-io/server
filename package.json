{
  "name": "sprova-server",
  "version": "0.1.0",
  "description": "Sprova Test Framework Server",
  "main": "src/server.ts",
  "scripts": {
    "test": "NODE_ENV=test tsc && jest --runInBand",
    "publish-coverage": "./node_modules/.bin/codecov --token=\"$CODECOV_TOKEN\"",
    "tsc": "tsc",
    "clean": "./node_modules/.bin/gulp clean",
    "lint-fix": "tslint -c tslint.json -p tsconfig.json --fix",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "dev": "nodemon",
    "prod": "tsc && ./node_modules/.bin/gulp copy-config && node ./build/src/server.js",
    "docs": "./node_modules/.bin/apidoc -i src/api -o docs/"
  },
  "author": "Aldi Alimucaj",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-winston": "^3.2.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongodb": "^3.2.7",
    "multer": "^1.4.2",
    "sprova-types": "^1.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/express-jwt": "0.0.42",
    "@types/express-winston": "^3.0.3",
    "@types/jest": "^24.0.16",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.136",
    "@types/mongodb": "^3.1.31",
    "@types/multer": "^1.3.7",
    "@types/node": "^12.6.8",
    "@types/supertest": "^2.0.8",
    "@types/winston": "^2.4.4",
    "apidoc": "^0.17.7",
    "codecov": "^3.5.0",
    "del": "^5.0.0",
    "gulp": "^4.0.2",
    "jest": "^24.8.0",
    "mongodb-memory-server": "^5.1.9",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "sinon": "^7.3.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  }
}
